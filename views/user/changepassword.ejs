    <style>
        .dashboard-container {
            max-width: 500px;
            margin: auto;
            padding: 20px;
            background: white; /* White background */
            border-radius: 8px;
            box-shadow: 0 0 10px rgba(0, 0, 0, 0.1); /* Light shadow */
        }

        .dashboard-header {
            text-align: center;
            margin-bottom: 20px;
        }

        .dashboard-header h1 {
            margin: 0;
            font-size: 24px;
            color: #333; /* Dark text */
        }

        .user-info {
            display: flex;
            flex-direction: column;
            gap: 15px;
        }

        .d-flex {
            display: flex;
        }

        .d-flex span {
            font-weight: bold;
            padding-right: 15px;
            color: #555; /* Slightly darker for emphasis */
        }

        .d-flex p {
            margin: 0;
            color: #333; /* Dark text */
        }

        .view i {
            margin-right: 5px; /* Space between icon and text */
        }

        .text-red {
            color: red !important;
            font-size: 12px;
            margin-top: 5px;
        }
        .form-background {
            background-color: #f9f9f9;
            border-radius: 8px;
            padding: 20px;
            box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
        }
        .default-form-box {
            margin-bottom: 20px;
        }
        .btn-black-default-hover {
            background-color: #000;
            color: #fff;
            border: none;
            padding: 10px 20px;
            cursor: pointer;
            border-radius: 4px;
            transition: background-color 0.3s;
        }
        .btn-black-default-hover:hover {
            background-color: #333;
        }

        .password-input {
            position: relative;
        }
        
        .password-toggle-button {
            position: absolute;
            top: 50%;
            transform: translateY(-50%);
            right: 10px;
            cursor: pointer;
        }

    </style>

