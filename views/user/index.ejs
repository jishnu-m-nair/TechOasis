        .instagram-image-link {
            flex: 1 0 calc(33.333% - 10px); /* Each image takes up roughly a third of the width */
            max-width: calc(33.333% - 10px); /* Prevent images from exceeding a third of the container */
            position: relative;
        }

        .instagram-image-link img {
            width: 100%;
            height: 200px; /* Set a fixed height */
            object-fit: cover; /* Ensures the image covers the container without distortion */
            display: block;
            border-radius: 5px; /* Optional: for rounded corners */
        }

        @media (max-width: 768px) {
            .instagram-image-link {
                flex: 1 0 calc(50% - 10px); /* Two images per row on smaller screens */
