    <style>
        .dashboard-container {
            max-width: 500px;
            margin: auto;
            padding: 20px;
            background: white; /* White background */
            border-radius: 8px;
            box-shadow: 0 0 10px rgba(0, 0, 0, 0.1); /* Light shadow */
        }

        .dashboard-header {
            text-align: center;
            margin-bottom: 20px;
        }

        .dashboard-header h1 {
            margin: 0;
            font-size: 24px;
            color: #333; /* Dark text */
        }

        .user-info {
            display: flex;
            flex-direction: column;
            gap: 15px;
        }

        .d-flex {
            display: flex;
        }

        .d-flex span {
            font-weight: bold;
            padding-right: 15px;
            color: #555; /* Slightly darker for emphasis */
        }

        .d-flex p {
            margin: 0;
            color: #333; /* Dark text */
        }

        .view i {
            margin-right: 5px; /* Space between icon and text */
        }
        .no-userinfo {
            padding: 20px;
            text-align: center;
            color: #ff0000; /* Red color for error or notice */
            background-color: #fff3f3; /* Light red background */
            border: 1px solid #ff0000; /* Red border */
            border-radius: 8px;
            margin-top: 20px;
        }
        address {
            font-style: normal;
            line-height: 1.5;
        }
        .address-container {
            margin: 20px;
            padding: 20px;
            border: 1px solid #ccc;
            border-radius: 5px;
        }
        .address-title {
            font-weight: bold;
            margin-top: 10px;
        }
        .address-list {
            list-style-type: none;
            padding: 0;
        }
        .address-item {
            margin-bottom: 15px;
            padding-bottom: 15px;
            border-bottom: 1px solid #ddd;
        }
        .address-item:last-child {
            border-bottom: none;
        }

        .new-address-button {
            display: inline-block;
            padding: 10px 20px;
            font-size: 16px;
            color: #fff;
            background-color: #007bff; /* Primary button color */
            border: none;
            border-radius: 5px;
            text-align: center;
            text-decoration: none;
            transition: background-color 0.3s ease;
        }

        .new-address-button:hover {
            background-color: #0056b3; /* Darker shade on hover */
        }

        .new-address-button:active {
            background-color: #00408d; /* Even darker shade on active click */
        }

    </style>

